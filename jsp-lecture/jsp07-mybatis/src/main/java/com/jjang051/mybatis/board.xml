<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.jjang051.mybatis">
	<select id="getAllReplyboard"
		resultType="com.jjang051.dto.BoardDto">
		select * from replyboard
	</select>
	<select id="getOneBoard"
		resultType="com.jjang051.dto.BoardDto" parameterType="int">
		select * from replyboard where no=#{no}
	</select>
</mapper>

